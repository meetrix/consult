{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension":0,
        "import/no-extraneous-dependencies":0,
        "react/prefer-stateless-function":0,
        "no-underscore-dangle":0,
        "no-console":1,
        "react/no-array-index-key":0
    },
    "globals": {
        "localStorage":true,
        "document":true,
        "window":true,
        "URLSearchParams":true,
        "Headers":true,
        "fetch":true
    }
}